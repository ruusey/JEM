{
	"version": "2.0.0",
	"configurations": [
		{
			"name": "Launch jem.js",
			"type": "node",
			"console": "integratedTerminal",
			"request": "launch",
			"program": "${workspaceFolder}/WebContent/js/jem.js",
			"cwd": "${workspaceFolder}/WebContent/js"
		},
		{
			"name": "Attach to Chrome JEM",
			"type": "chrome",
			"request": "launch",
			"webRoot": "${workspaceFolder}",
			"url": "https://uscworks.com/JEM"
		}
	]
}